*/

"[p r]" = [p r]
;
"[p=r]" = [p r]     
           0 0
           1 1
           2 2
;
  

"[p<r]" = [p r]     
           0 0
           0 1
           0 2
           1 1
           1 2
           2 2
;

"[q r]" = [q r]
;
"[q r]a0" = [q r]
      a 0
;
"[q r]b0" = [q r]
      b 0
;
"[q r]a0b0" = [q r]
      a 0
      b 0
;
"[q r]a0b0a1" = [q r]
      a 0
      b 0
      a 1
;
"[q r]a0b0a1b1" = [q r]
      a 0
      b 0
      a 1
      b 1
;


"[p q]" = [p q]
;


"p->q"=[p  q  r]
   0  a  0
   0  a  1
   1  c  0
