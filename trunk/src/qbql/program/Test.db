ABC = [m  _s]
       a "{a,b,c}"
       b "{a,b,c}"
       c "{a,b,c}"
;
A = [m  _s]
     a "{a}"
;
B = [m  _s]
     b "{b}"
;
C = [m  _s]
     c "{c}"
;
AB = [m  _s]
      a "{a,b}"
      b "{a,b}"
;
BC = [m  _s]
      c "{b,c}"
      b "{b,c}"
;
EmptySet = [_s] "{}";

Sets = ABC v AB v A v B v C v BC;
Sets1 = Sets /^ [_s=s1];

